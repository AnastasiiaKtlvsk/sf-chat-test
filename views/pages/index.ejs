<script type="text/javascript" charset="UTF-8" >

document.onclick = function(event) {
	var node = event.target;
	var parentNode = node.parentNode;
	var grandparentNode = (parentNode != null) ? parentNode.parentNode : null;

	var tagName = event.target.tagName;
	var sendButtonClass = 'chatActionButton Upload this file medium uiButton--default uiButton embeddedServiceLiveAgentStateChatAction';

	if (tagName == "EMBEDDEDSERVICE-CHAT-HEADER") {
		var counter = document.getElementsByClassName("dot")[0];
		if (counter) {
			counter.style.visibility = "hidden";
			document.title = title;
		}
	} else if (node.className == sendButtonClass || node.getAttribute('data-key') == 'paper_plane' || 
	(parentNode != null && parentNode.nodeName != '#document-fragment' && (parentNode.className == sendButtonClass || parentNode.getAttribute('data-key') == 'paper_plane' ))|| 
	(grandparentNode != null && grandparentNode.nodeName != '#document-fragment' &&  (grandparentNode.className == sendButtonClass || grandparentNode.getAttribute('data-key') == 'paper_plane' ))){
		embedded_svc.liveAgentAPI.sendCustomEvent(
		'type:Customer_to_agent_event_type',
		'File uploaded');
	} else if ( node.className == 'slds-button slds-button_neutral customStartButton customChatEndButton') {
		embedded_svc.liveAgentAPI.endChat();
	}
}

document.addEventListener(
		"setCounter",
		function(event) {
			var counter = document.getElementsByClassName("dot")[0];
			if (counter) {
				counter.style.visibility = "visible";
			}
		},
		false
);


document.addEventListener(
	"setCustomField",
	function(event) {
		var detail = event.detail;
		console.log('detail = ', detail);
		embedded_svc.settings.extraPrechatFormDetails[0].value = event.detail.caseid;
		embedded_svc.settings.extraPrechatFormDetails[1].value = event.detail.name;
		embedded_svc.settings.extraPrechatFormDetails[2].value = event.detail.email;
		embedded_svc.settings.extraPrechatFormDetails[3].value = event.detail.phone;
		embedded_svc.settings.extraPrechatFormDetails[4].value = event.detail.subject;
		embedded_svc.settings.extraPrechatFormDetails[5].value = event.detail.agentreason;
		embedded_svc.settings.extraPrechatFormDetails[6].value = event.detail.chatdialog;
		event.detail.callback();
	},
	false
);

var title = document.title;

	var accountId  = new URLSearchParams(window.location.search).get('accountID');
